{
  "singleQuote": true,
  "semi": true,
  "printWidth": 130,
  "jsxBracketSameLine": true,
  "svelteSortOrder": "options-scripts-styles-markup",
  "svelteBracketNewLine": false,
  "bracketSameLine": true,
  "tabWidth": 2,
  "useTabs": true,
  "plugins": [ "prettier-plugin-svelte", "prettier-plugin-tailwindcss" ],
  "pluginSearchDirs": [ "." ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
